<?xml version="1.0"?>

<PropertyList>

 <generic>

  <output>
   <line_separator>newline</line_separator>
   <var_separator>,</var_separator>

   <chunk>
    <name>indicated speed (kt)</name>
    <type>double</type>
    <format>%.4e</format>
    <node>/instrumentation/airspeed-indicator/indicated-speed-kt</node>
   </chunk>

   <chunk>
    <name>pitch att (deg)</name>
    <type>float</type>
    <format>%.1f</format>
    <node>/orientation/pitch-deg</node>
   </chunk>

   <chunk>
    <name>Roll att (deg)</name>
    <type>float</type>
    <format>%.1f</format>
    <node>/orientation/roll-deg</node>
   </chunk>

   <chunk>
    <name>magnetic heading (deg)</name>
    <type>float</type>
    <format>%.1f</format>
    <node>/orientation/heading-magnetic-deg</node>
   </chunk>

   <chunk>
    <name>Altitude</name>
    <type>double</type>
    <format>%.4e</format>
    <node>/instrumentation/altimeter/pressure-alt-ft</node>
   </chunk>

   <chunk>
    <name>clock</name>
    <type>string</type>
    <format>%s</format>
    <node>/instrumentation/clock/indicated-string</node>
   </chunk>

   <chunk>
    <name>Voltage</name>
    <type>float</type>
    <format>%.4e</format>
    <node>/systems/electrical/volts</node>
   </chunk>

   <chunk>
    <name>RPM</name>
    <type>double</type>
    <format>%.4e</format>
    <node>/engines/engine/rpm</node>
   </chunk>

   <chunk>
    <name>Oil Temp F</name>
    <type>float</type>
    <format>%.4e</format>
    <node>/engines/engine[0]/oil-temperature-degf</node>
   </chunk>

   <chunk>
    <name>Oil Pressure</name>
    <type>float</type>
    <format>%.4e</format>
    <node>/engines/engine[0]/oil-pressure-psi</node>
   </chunk>

   <chunk>
    <name>EGT</name>
    <type>double</type>
    <format>%.4e</format>
    <node>/engines/engine/egt-degf-fix</node>
   </chunk>

   <chunk>
    <name>Fuel Flow</name>
    <type>double</type>
    <format>%.4e</format>
    <node>/engines/engine/fuel-flow-gph</node>
   </chunk>

   <chunk>
    <name>Fuel Tank 0</name>
    <type>float</type>
    <format>%.4e</format>
    <node>/consumables/fuel/tank[0]/level-gal_us</node>
   </chunk>

   <chunk>
    <name>Fuel Tank 1</name>
    <type>float</type>
    <format>%.4e</format>
    <node>/consumables/fuel/tank[1]/level-gal_us</node>
   </chunk>

  </output>

 </generic>

</PropertyList>
